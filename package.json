{
	"name": "disdogs",
	"version": "0.0.1",
	"description": "Dogs",
	"main": "dist/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc -b src",
		"start": "pm2-runtime start ecosystem.config.js --env production",
		"dev": "node-dev --respawn src/index.ts | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
		"update": "yarn upgrade-interactive",
		"lint": "eslint --fix --ext ts src",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.{js,ts,json}\"",
		"heroku-postbuild": "yarn add pm2"
	},
	"author": "CharmedGoose",
	"license": "MIT",
	"dependencies": {
		"@sapphire/framework": "^2.5.1",
		"@sapphire/ts-config": "^3.3.4",
		"discord.js": "^13.8.0",
		"dotenv": "^16.0.1",
		"mongoose": "^6.3.8",
		"pino": "7.0.0-rc.9",
		"pm2": "^5.2.0",
		"@types/node": "^18.0.0",
		"typescript": "^4.7.3"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/prettier-config": "^1.4.3",
		"@swc/core": "^1.2.204",
		"@swc/helpers": "^0.4.2",
		"eslint": "^8.17.0",
		"node-dev": "^7.4.3",
		"prettier": "^2.7.1",
		"regenerator-runtime": "^0.13.9",
		"ts-node": "^10.8.1"
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"engines": {
		"node": "18.3.0"
	},
	"packageManager": "yarn@3.2.1"
}
