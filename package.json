{
	"name": "disdogs",
	"version": "0.0.1",
	"description": "Dogs",
	"main": "dist/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc -b src",
		"start": "node --enable-source-maps dist/index.js",
		"dev": "nodemon -e ts --watch src .env --exec \"tsc -b src --incremental && node ./dist/index.js\" | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
		"update": "yarn upgrade-interactive",
		"lint": "eslint --fix --ext ts src",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.{js,ts,json}\""
	},
	"author": "CharmedGoose",
	"license": "MIT",
	"dependencies": {
		"@napi-rs/canvas": "^0.1.30",
		"@sapphire/discord.js-utilities": "^4.11.3",
		"@sapphire/framework": "^3.0.0-next.99b4446.0",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^18.0.0",
		"discord-api-types": "^0.33.0",
		"discord.js": "^13.8.1",
		"dotenv": "^16.0.1",
		"mongoose": "^6.4.1",
		"pino": "8.1.0",
		"ts-loader": "^9.3.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/prettier-config": "^1.4.3",
		"@swc/core": "^1.2.207",
		"@swc/helpers": "^0.4.3",
		"eslint": "^8.18.0",
		"nodemon": "^2.0.18",
		"prettier": "^2.7.1",
		"regenerator-runtime": "^0.13.9",
		"ts-node": "^10.8.1",
		"ts-node-dev": "^2.0.0"
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"engines": {
		"node": "16.3.0"
	},
	"packageManager": "yarn@3.2.1"
}
